            }

            input[type=range] {
                height: 41px;
                -webkit-appearance: none;
                margin: 8px 0;
                margin-left: 8px;
                transition: 0.4s;
                background-color: transparent;
                flex-grow: 2;
            }

            input[type=range]:disabled {
                cursor: not-allowed;
            }

            input[type=range]:focus {
                outline: none;
            }

            input[type=range]::-webkit-slider-runnable-track {
                height: 15px;
                cursor: pointer;
                transition: 0.2s;
                box-shadow: 0px 0px 0px #000000;
                background: #59BAD9;
                border-radius: 5px;
                border: 0px solid #000000;
            }

            input[type=range]:disabled::-webkit-slider-runnable-track {
                cursor: not-allowed;
                opacity: 0.75;
            }

            input[type=range]::-webkit-slider-thumb {
                box-shadow: 0px 0px 0px #000000;
                border: 0px solid #000000;
                height: 35px;
                width: 20px;
                border-radius: 5px;
                background: #CFDE9E;
                cursor: pointer;
                -webkit-appearance: none;
                margin-top: -10px;
            }

            input[type=range]:disabled::-webkit-slider-thumb {
                cursor: not-allowed;
            }

            input[type=range]:focus::-webkit-slider-runnable-track {
                background: #59BAD9;
            }

            input[type=range]::-moz-range-track {
                height: 15px;
                cursor: pointer;
                transition: 0.2s;
                box-shadow: 0px 0px 0px #000000;
                background: #59BAD9;
                border-radius: 5px;
                border: 0px solid #000000;
            }

            input[type=range]:disabled::-moz-range-track {
                cursor: not-allowed;
                background: #59BAD973;
            }

            input[type=range]::-moz-range-thumb {
                box-shadow: 0px 0px 0px #000000;
                border: 0px solid #000000;
                height: 35px;
                width: 20px;
                border-radius: 5px;
                background: #CFDE9E;
                cursor: pointer;
            }

            input[type=range]:disabled::-moz-range-thumb {
                cursor: not-allowed;
            }

            input[type=range]::-ms-track {
                height: 15px;
                cursor: pointer;
                transition: 0.2s;
                background: transparent;
                border-color: transparent;
                color: transparent;
            }

            input[type=range]:disabled::-ms-track {
                cursor: not-allowed;
            }

            input[type=range]::-ms-fill-lower {
                background: #59BAD9;
                border: 0px solid #000000;
                border-radius: 10px;
                box-shadow: 0px 0px 0px #000000;
            }

            input[type=range]:disabled::-ms-fill-lower , input[type=range]:disabled::-ms-fill-upper {
                opacity: 0.75;
            }

            input[type=range]::-ms-fill-upper {
                background: #59BAD9;
                border: 0px solid #000000;
                border-radius: 10px;
                box-shadow: 0px 0px 0px #000000;
            }

            input[type=range]::-ms-thumb {
                margin-top: 1px;
                box-shadow: 0px 0px 0px #000000;
                border: 0px solid #000000;
                height: 35px;
                width: 20px;
                border-radius: 5px;
                background: #CFDE9E;
                cursor: pointer;
            }

            input[type=range]:disabled::-ms-thumb {
                cursor: not-allowed;
            }

            input[type=range]:focus::-ms-fill-lower {
                background: #59BAD9;
            }

            input[type=range]:focus::-ms-fill-upper {
                background: #59BAD9;
            }

            .slidecontainer input[type=number] {
                background: white;
                color: white;
                padding: 6px 8px;
                text-align: center;
                text-decoration: none;
                display: inline-block;
                font-size: 16px;
                margin: 4px 2px;
                cursor: pointer;
                border-radius: 12px;
                width: 40px;
                transition: 0.4s;
                color: black;
                border: 2px solid #e7e7e7;
                font-weight: 600;
            }

            .slidecontainer input[type=number]:disabled {
                cursor: not-allowed;
                background-color: #e3e3e3;
            }

            .slidecontainer {
                display: flex;
                align-items: center;
                justify-content: space-between;
            }

            input[type=number]::-webkit-inner-spin-button {
                opacity: 1;
            }

            /* width */
            ::-webkit-scrollbar {
                width: 30px;
                cursor: pointer;
            }

            /* Track */
            ::-webkit-scrollbar-track {
                background: #848484;
            }

            /* Handle */
            ::-webkit-scrollbar-thumb {
                background: #59BAD9;
            }

            /* Handle on hover */
            ::-webkit-scrollbar-thumb:hover {
                background: #5BB7D2;
            }

            #time {
                font-family: Courier, monospace;
                font-size: 40px;
            }

            .bottomText {
                margin: 0.3em 10em;
            }

            .time-headers {
                font-size: 36px;
            }

            @media only screen and (max-width: 1000px) {
                .scorer {
                    grid-template-columns: 1fr;
                    grid-template-rows: auto auto auto;
                    grid-template-areas: "column1" "column2" "column3";
                }

                .timer-and-buttons {
                    flex-direction: column
                }

                .everything {
                    grid-template-rows: 225px 1fr;
                }

                .timer h1, .stopwatch h1 {
                    display: none;
                }

                #time {
                    display: initial;
                }

                .everything {
                    padding: 0;
                }
                .bottomText {
                    margin: 0.3em 0.3em;
                }
            }
        </style>
