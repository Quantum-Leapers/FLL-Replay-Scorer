            .name {
                background-color: #59BAD9;
                border-top-left-radius: 20px;
                border-top-right-radius: 20px;
                padding: 8px 10px;
                font-size: 20px;
                margin: 0;
                color: white;
            }

            .card {
                border-radius: 20px;
                background-color: #848484;
                margin: 10px;
            }

            .base-timer {
                position: relative;
                width: 90px;
                height: 90px;
                margin: 0 20px;
            }

            .base-timer__svg {
                transform: scaleX(-1);
            }

            .base-timer__circle {
                fill: none;
                stroke: none;
            }

            .base-timer__path-elapsed {
                stroke-width: 10px;
                stroke: grey;
            }

            .base-timer__path-remaining {
                stroke-width: 10px;
                stroke-linecap: round;
                transform: rotate(90deg);
                transform-origin: center;
                transition: 1s linear all;
                fill-rule: nonzero;
                stroke: currentColor;
            }

            .base-timer__path-remaining.green {
                color: rgb(65, 184, 131);
            }

            .base-timer__path-remaining.orange {
                color: orange;
            }

            .base-timer__path-remaining.red {
                color: red;
            }

            .base-timer__label {
                position: absolute;
                width: 90px;
                height: 90px;
                top: 0;
                display: flex;
                align-items: center;
                justify-content: center;
                font-size: 28px;
            }

            .button {
                background: white;
                color: white;
                padding: 6px 8px;
                text-align: center;
                text-decoration: none;
                display: inline-block;
                font-size: 16px;
                margin: 4px 2px;
                cursor: pointer;
                border-radius: 12px;
                width: 80px;
                transition: 0.4s;
                color: black;
                border: 2px solid #e7e7e7;
                font-weight: 600;
            }

            .button:disabled {
                cursor: not-allowed;
                background-color: #e3e3e3;
            }

            .button:disabled:hover {
                background-color: #e3e3e3;
            }

            .button1 {
                color: black;
                border: 2px solid #e7e7e7;
            }

            .button:active, .button:focus, .button:visited {
