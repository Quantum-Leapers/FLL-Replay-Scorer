            .goog-te-gadget .goog-te-combo {
                font-family: "GT-Walsheim-Pro",Graphik,-apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Arial,"Noto Sans",sans-serif,"Apple Color Emoji","Segoe UI Emoji","Segoe UI Symbol","Noto Color Emoji";
            }

            .goog-logo-link {
                font-weight: normal;
            }

            body {
                margin: 0;
            }

            html {
                scroll-behavior: smooth;
                scrollbar-color: #59BAD9 #848484;
            }

            .everything {
                display: grid;
                height: 100vh;
                place-items: center;
                grid-template-columns: 100%;
                grid-template-rows: 100px auto 1fr;
                gap: 1px 1px;
                grid-template-areas: "timer-and-buttons" "title" "scorer";
                padding: 0 15px;
            }

            .title {
                text-align: center;
                grid-area: title;
                margin: 15px 0 5px 0;
            }

            .title h1 {
                margin: 0;
                font-size: 40px;
                font-weight: 800;
            }

            .buttons-container {
                text-align: center;
                margin: 6px 0;
            }

            .timer-and-buttons {
                grid-area: timer-and-buttons;
                display: flex;
                align-items: center;
                justify-content: space-around;
                width: 100%;
            }

            .timer {
                display: flex;
                align-items: center;
            }

            .stopwatch {
                display: flex;
                align-items: center;
                height: 100px;
            }

            .scorer {
                grid-area: scorer;
                display: grid;
                grid-template-columns: 1fr 1fr 1fr;
                grid-template-rows: 100%;
                width: 100%;
                grid-template-areas: "column1 column2 column3";
            }

            .column1 {
                grid-area: column1;
                height: 100%;
            }

            .column2 {
                grid-area: column2;
                height: 100%;
            }

            .column3 {
                grid-area: column3;
                height: 100%;
            }

            .card-body {
                padding: 8px;
            }

            .card-body p {
                color: white;
                margin: 0;
            }

            .playpause label {
                display: block;
                box-sizing: border-box;
                width: 0;
                height: 74px;
                outline: none;
                border-color: transparent transparent transparent black;
                transition: 400ms all ease;
                cursor: pointer;
                border-style: solid;
                outline: none;
                border-width: 37px 0 37px 60px;
            }

            .playpause label:hover {
                border-color: transparent transparent transparent #59BAD9;
            }

            .playpause label:active {
                transition: 100ms all ease;
            }

            .playpause input[type="checkbox"] {
                position: absolute;
                left: -9999px;
                outline: none;
            }

            .playpause input[type="checkbox"]:checked + label {
                border-style: double;
                outline: none;
                border-width: 0px 0 0px 60px;
            }

            .playpause input[type="checkbox"]:focus + label {
                outline: none;
            }

            .stopwatch-playpause label {
                display: block;
                box-sizing: border-box;
                width: 0;
                height: 74px;
                outline: none;
                border-color: transparent transparent transparent black;
                transition: 400ms all ease;
                cursor: pointer;
                border-style: solid;
                outline: none;
                border-width: 37px 0 37px 60px;
            }

            .stopwatch-playpause label:hover {
                border-color: transparent transparent transparent #59BAD9;
            }

            .stopwatch-playpause label:active {
                transition: 100ms all ease;
            }

            .stopwatch-playpause input[type="checkbox"] {
                position: absolute;
                left: -9999px;
                outline: none;
            }

            .stopwatch-playpause input[type="checkbox"]:checked + label {
                border-style: double;
                outline: none;
                border-width: 0px 0 0px 60px;
